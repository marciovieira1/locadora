<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Locadora.Domain">
  <class name="Locadora.Domain.TUser, Locadora.Model" table="t_users" schema="dbo" >
        <id name="Id" column="id">
      <generator class="native" />
    </id>
         
    <property name="Name">
      <column name="name" length="255" sql-type="nvarchar" />
    </property>
    <property name="Password">
      <column name="password" length="48" sql-type="varbinary" />
    </property>
    <property name="EnumProfileUser">
      <column name="enum_profile_user" length="4" sql-type="int" />
    </property>
    <property name="Login">
      <column name="login" length="255" sql-type="nvarchar" />
    </property>


  </class>
</hibernate-mapping>